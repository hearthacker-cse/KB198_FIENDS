<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_61sdp"
    android:background="@color/colorLightPurpleTrans"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/imageView5"
        android:layout_width="@dimen/_9sdp"
        android:layout_height="@dimen/_9sdp"
        android:layout_marginStart="@dimen/_6sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:background="@color/colorLightPurpleTrans"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/near_by_zone_disease_name"
        android:layout_width="@dimen/_107sdp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_6sdp"
        android:background="@color/colorLightPurpleTrans"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/near_by_zone_disease_discription"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/_4sdp"
        android:layout_marginEnd="@dimen/_6sdp"
        android:layout_marginBottom="@dimen/_4sdp"
        android:background="@color/colorLightPurpleTrans"
        app:layout_constraintBottom_toTopOf="@+id/start_date_lable"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/near_by_zone_disease_name" />

    <View
        android:id="@+id/start_date_lable"
        android:layout_width="@dimen/_36sdp"
        android:layout_height="@dimen/_11sdp"
        android:layout_marginBottom="@dimen/_6sdp"
        android:background="@color/colorLightPurpleTrans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/near_by_zone_disease_discription" />

    <View
        android:id="@+id/start_date"
        android:layout_width="@dimen/_54sdp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_4sdp"
        android:background="@color/colorLightPurpleTrans"
        app:layout_constraintBottom_toBottomOf="@+id/start_date_lable"
        app:layout_constraintStart_toEndOf="@+id/start_date_lable"
        app:layout_constraintTop_toTopOf="@+id/start_date_lable" />

    <View
        android:id="@+id/diatance"
        android:layout_width="@dimen/_54sdp"
        android:layout_height="@dimen/_11sdp"
        android:layout_marginBottom="@dimen/_6sdp"
        android:background="@color/colorLightPurpleTrans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/near_by_zone_disease_discription" />

    <View
        android:id="@+id/textView15"
        android:layout_width="@dimen/_36sdp"
        android:layout_height="0dp"
        android:layout_marginEnd="@dimen/_4sdp"
        android:background="@color/colorLightPurpleTrans"
        app:layout_constraintBottom_toBottomOf="@+id/diatance"
        app:layout_constraintEnd_toStartOf="@+id/diatance"
        app:layout_constraintTop_toTopOf="@+id/diatance" />

</androidx.constraintlayout.widget.ConstraintLayout>